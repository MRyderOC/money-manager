{
    "base": {
        "expense": {
            "columns": [
                "Description", "Amount", "Date",
                "Institution", "AccountName",
                "InstitutionCategory", "MyCategory",
                "IsTransfer", "IsValid"
            ],
            "read_args": {
                "header": 0,
                "parse_dates": ["Date"],
                "names": null
            },
            "validation_data": {
                "schema": {
                    "Amount": "float",
                    "Date": "datetime",
                    "isValid": "bool"
                },
                "column_values": {
                    "Amount": {"values": "^-?\\d+[.]\\d{1,2}$", "mode": "regex"},
                    "IsTransfer": {"values": ["expense", "transfer", "consider", "redundant"], "mode": "superset"},
                    "IsValid": {"values": [true, false], "mode": "equal"}
                }
            }
        }
    },


    "amex": {
        "credit": {
            "columns": [
                "Date",
                "Description",
                "Amount",
                "Extended Details",
                "Appears On Your Statement As",
                "Address",
                "City/State",
                "Zip Code",
                "Country",
                "Reference",
                "Category"
            ],
            "read_args": {
                "header": 0,
                "parse_dates": ["Date"],
                "names": null
            },
            "validation_data": {
                "schema": {
                    "Date": "datetime64_ns",
                    "Description": "string",
                    "Amount": "float",
                    "Category": "string"
                },
                "column_values": {
                    "Amount": {"values": "^-?\\d+[.]\\d{1,2}$", "mode": "regex"}
                }
            }
        }
    },
    "capitalone": {
        "credit": {
            "columns": [
                "Transaction Date",
                "Posted Date",
                "Card No.",
                "Description",
                "Category",
                "Debit",
                "Credit"
            ],
            "read_args": {
                "header": 0,
                "parse_dates": ["Transaction Date", "Posted Date"],
                "names": null
            },
            "validation_data": {
                "schema": {
                    "Transaction Date": "datetime64_ns",
                    "Posted Date": "datetime64_ns",
                    "Card No.": "int64",
                    "Debit": "float",
                    "Credit": "float"
                },
                "column_values": {
                    "Card No.": {"values": "^\\d{4}$", "mode": "regex"},
                    "Debit": {"values": "^\\d+[.]\\d{1,2}$", "mode": "regex", "na_action": "ignore"},
                    "Credit": {"values": "^\\d+[.]\\d{1,2}$", "mode": "regex", "na_action": "ignore"}
                }
            }
        }
    },
    "chase": {
        "credit": {
            "columns": [
                "Transaction Date",
                "Post Date",
                "Description",
                "Category",
                "Type",
                "Amount",
                "Memo"
            ],
            "read_args": {
                "header": 0,
                "parse_dates": ["Transaction Date", "Post Date"],
                "names": null
            },
            "validation_data": {
                "schema": {
                    "Transaction Date": "datetime64_ns",
                    "Post Date": "datetime64_ns",
                    "Amount": "float"
                },
                "column_values": {
                    "Type": {"values": ["Sale", "Payment"], "mode": "superset"},
                    "Amount": {"values": "^-?\\d+[.]\\d{1,2}$", "mode": "regex"}
                }
            }
        }
    },
    "citi": {
        "credit": {
            "columns": [
                "Status",
                "Date",
                "Description",
                "Debit",
                "Credit"
            ],
            "read_args": {
                "header": 0,
                "parse_dates": ["Date"],
                "names": null
            },
            "validation_data": {
                "schema": {
                    "Date": "datetime64_ns",
                    "Debit": "float",
                    "Credit": "float"
                },
                "column_values": {
                    "Status": {"values": ["Cleared"], "mode": "superset"},
                    "Debit": {"values": "^\\d+[.]\\d{1,2}$", "mode": "regex", "na_action": "ignore"},
                    "Credit": {"values": "^-\\d+[.]\\d{1,2}$", "mode": "regex", "na_action": "ignore"}
                }
            }
        }
    },
    "discover": {
        "credit": {
            "columns": [
                "Trans. Date",
                "Post Date",
                "Description",
                "Amount",
                "Category"
            ],
            "read_args": {
                "header": 0,
                "parse_dates": ["Trans. Date", "Post Date"],
                "names": null
            },
            "validation_data": {
                "schema": {
                    "Trans. Date": "datetime64_ns",
                    "Amount": "float"
                },
                "column_values": {
                    "Amount": {"values": "^-?\\d+[.]\\d{1,2}$", "mode": "regex"}
                }
            }
        }
    },
    "paypal": {
        "3rdparty": {
            "columns": [
                "Date",
                "Time",
                "TimeZone",
                "Name",
                "Type",
                "Status",
                "Currency",
                "Amount",
                "Receipt ID",
                "Balance"
            ],
            "read_args": {
                "header": 0,
                "parse_dates": ["Date"],
                "names": null
            },
            "validation_data": {
                "schema": {"Date": "datetime64_ns"},
                "column_values": {
                    "Currency": {"values": ["USD"], "mode": "equal"},
                    "Amount": {"values": "^-?\\d+(,\\d{3})*[.]\\d{1,2}$", "mode": "regex", "na_action": "ignore"}
                }
            }
        }
    },
    "venmo": {
        "3rdparty": {
            "columns": [
                "Unnamed: 0",
                "ID",
                "Datetime",
                "Type",
                "Status",
                "Note",
                "From",
                "To",
                "Amount (total)",
                "Amount (tip)",
                "Amount (fee)",
                "Funding Source",
                "Destination",
                "Beginning Balance",
                "Ending Balance",
                "Statement Period Venmo Fees",
                "Terminal Location",
                "Year to Date Venmo Fees",
                "Disclaimer"
            ],
            "read_args": {
                "header": 2,
                "parse_dates": ["Datetime"],
                "names": null
            },
            "validation_data": {
                "schema": {
                    "Datetime": "datetime64_ns"
                },
                "column_values": {
                    "Type": {"values": ["Payment", "Charge", "Merchant Transaction", "Standard Transfer"], "mode": "superset"},
                    "Status": {"values": ["Issued", "Complete"], "mode": "superset"},
                    "Amount (total)": {"values": "^[+|-] [$]\\d+(,\\d{3})*[.]\\d{1,2}$", "mode": "regex"}
                }
            }
        }
    }
}